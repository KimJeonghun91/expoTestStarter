{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["node", "@wdio/globals/types", "@wdio/mocha-framework", "@types/jest"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
